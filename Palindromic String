import java.util.*;
class TestClass {
    public static void main(String args[] ) throws Exception {
    Scanner s = new Scanner(System.in);
        String name = s.nextLine(); 
        String pal="";
        int l=name.length();
        while(l>0)
        {
            pal+=name.charAt(l-1);
            l--;
        }
        if(pal.equals(name))
        System.out.println("YES");
        else
        System.out.println("NO");
    }
}
